Map {
  background-color: #b8dee6;
}

#countries {
  ::outline {
    line-color: #85c5d3;
    line-width: 2;
    line-join: round;
  }
  polygon-fill: #fff;
}

#dfc50 {
  line-color:#594;
  line-width:0.5;
  polygon-opacity:1;
  polygon-fill:#ae8;
}


#DFCC {
  line-color:#594;
  line-width:0.5;
  polygon-opacity:1;
  polygon-fill:#ae8;
}


#dfcframework {
  line-color:#594;
  line-width:5;
  polygon-opacity:1;
  polygon-fill:#ae8;
  [ZONE="MP"]{polygon-fill:#FF3366}
  [ZONE="MV2"]{polygon-fill:#FF6633}
  [ZONE="IS"]{polygon-fill:#F5B800}
}
